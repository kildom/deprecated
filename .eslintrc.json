{
    "parserOptions": {
        "ecmaVersion" : 3
    },
    "extends": "eslint:recommended",
    "rules": {
        "indent": [ "error", 4 ],
        "array-bracket-spacing": ["error", "always"],
        "no-trailing-spaces": "error",
        "no-redeclare": "off",
        "quotes": ["error", "single"],
        "brace-style": ["error", "allman", { "allowSingleLine": true }],
        "no-unused-vars": ["error", { "args": "none" }]
    }
}
